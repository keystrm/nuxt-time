{
  "name": "nuxt-time",
  "version": "0.1.3",
  "license": "MIT",
  "description": "Safely render local datesÂ and times on the server with Nuxt 3",
  "repository": "danielroe/nuxt-time",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "date"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "pnpm nuxt-module-build build --stub && pnpm nuxt-module-build prepare && nuxi prepare playground",
    "docs:dev": "nuxi dev docs",
    "docs:build": "nuxi generate docs",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "prepare": "husky install",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "vitest run",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.0",
    "defu": "^6.1.4",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@nuxt/module-builder": "0.5.5",
    "@nuxt/schema": "3.11.0",
    "@nuxt/test-utils": "3.12.0",
    "@types/node": "20.11.30",
    "@vitest/coverage-v8": "1.4.0",
    "@vue/test-utils": "2.4.5",
    "bumpp": "9.4.0",
    "esbuild": "0.20.2",
    "eslint": "9.0.0",
    "expect-type": "0.18.0",
    "get-port-please": "3.1.2",
    "happy-dom": "14.1.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "magic-regexp": "0.8.0",
    "nuxt": "3.11.0",
    "playwright": "1.42.1",
    "typescript": "5.4.5",
    "vite": "5.1.6",
    "vitest": "1.4.0",
    "vitest-environment-nuxt": "1.0.0",
    "vue": "3.4.21",
    "@nuxt/eslint-config": "0.3.6"
  },
  "resolutions": {
    "@nuxt/kit": "3.11.0",
    "nuxt-time": "link:."
  },
  "packageManager": "pnpm@8.15.7"
}
